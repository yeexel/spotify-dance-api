option_settings:
  - option_name: NODE_ENV
    value: production
  - option_name: API_URL
    value: https://sdapi.yeexel.com
  - option_name: CLIENT_URL
    value: https://spotify-dance-client.now.sh
  - option_name: SPOTIFY_CLIENT_ID
    value: <PLACEHOLDER>
  - option_name: SPOTIFY_CLIENT_SECRET
    value: <PLACEHOLDER>
  - option_name: JWT_SECRET
    value: <PLACEHOLDER>
  - option_name: TYPEORM_CONNECTION
    value: postgres
  - option_name: TYPEORM_HOST
    value: <PLACEHOLDER>
  - option_name: TYPEORM_USERNAME
    value: <PLACEHOLDER>
  - option_name: TYPEORM_PASSWORD
    value: <PLACEHOLDER>
  - option_name: TYPEORM_DATABASE
    value: <PLACEHOLDER>
  - option_name: TYPEORM_ENTITIES
    value: build/entity/**/*.js
  - option_name: TYPEORM_MIGRATIONS
    value: build/migrations/**/*.js
